<Application x:Class="DeviationModule.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DeviationModule"
             xmlns:vm="clr-namespace:DeviationModule.ViewModel"
             xmlns:view="clr-namespace:DeviationModule.Views"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="vm:PositionViewModel">
            <view:PositionsView></view:PositionsView>
        </DataTemplate>
        <DataTemplate DataType="vm:PlaningViewModel">
            <view:PlaningView></view:PlaningView>
        </DataTemplate>
        <DataTemplate DataType="vm:ApplicationViewModel">
            <view:EditorView></view:EditorView>
        </DataTemplate>
    </Application.Resources>
</Application>
